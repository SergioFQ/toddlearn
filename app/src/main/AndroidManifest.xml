<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.g12.toddlearn">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".app.MyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Game2Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Game1Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".NewPasswordActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".DataChangeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ChildProgressActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".RegistrationActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".SettingMenuActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".GameSelectionActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" >
            android:screenOrientation="portrait"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>